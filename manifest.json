{
    "name": "Euisu",
    "version": "0.0.1",
    "description": "Yeongho's custom features",
    "permissions": [
        "activeTab",
        "clipboardWrite"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://en.dict.naver.com/*"
            ],
            "run_at": "document_idle",
            "js": [
                "content_scripts/naver_en_dict.js"
            ]
        }
    ],
    "manifest_version": 2
}